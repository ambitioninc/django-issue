[run]
branch = True
omit =
    issue/migrations/*
    issue/south_migrations/*
    issue/tests/*
    issue/version.py
    issue/apps.py
source = issue
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1
